                            var formatter = new Intl.NumberFormat('vi-VN', {
                                style: 'currency',
                                currency: 'VND',
                                minimumFractionDigits: 0,
                            });

                            var swiper = new Swiper('.block-hotsale .box-products .swiper-container', {
                                slidesPerView: 5,
                                slidesPerGroup: 5,
                                spaceBetween: 10,
                                loopFillGroupWithBlank: true,
                                lazy: {
                                    loadPrevNext: true,
                                },
                                navigation: {
                                    nextEl: '.block-hotsale .box-products .swiper-button-next',
                                    prevEl: '.block-hotsale .box-products .swiper-button-prev',
                                },
                            });

                            jQuery.ajax({
                                method: "GET",
                                url: "https://cellphones.com.vn/ltpg/product/loadProductAjax/?product_group_ud=pk_flash_sale",
                                dataType: "json",
                                success: function (response) {
                                    if (response.product_return > 0) {
                                        const list_product = response.data.products;
                                        list_product.sort(compare);

                                        const from_date = response.data.countdown_date_utc;
                                        const to_date = response.data.to_date_utc;

                                        let is_sale = setCountDown(from_date, to_date);

                                        let length = 0;
                                        jQuery.each(list_product, function (index, product) {
                                            if (product.price != 0 && jQuery.inArray("610", product.categories) >= 0) {
                                                var id = product['entity_id'];
                                                var name = product['name'];
                                                var img = product['small_image'];
                                                var url = product['url'];

                                                var old_price = product['price'];
                                                var special_price = product['sale_price' + store_id];

                                                if (special_price <= 0 && old_price > 0) {
                                                    special_price = old_price;
                                                }

                                                var countAllReviews = product['countAllReview'];
                                                var averageRatings = product['averageRatings'];

                                                var percent = 0;
                                                if (old_price > 0) {
                                                    percent = Math.round(((old_price - special_price) / old_price) * 100);
                                                }

                                                var key_selling_point = '';

                                                html += '<div id="' + id + '" class="swiper-slide item-product">';
                                                html += '<div class="item-product__box-img">';
                                                html += '<a href="' + url + '" rel="nofollow">';
                                                html += '<img class="swiper-lazy" data-src="' + img + '" alt="' + name + '"/>';
                                                html += '</a>';
                                                html += '</div>';
                                                if (percent > 2) {
                                                    html += '<div class="item-product__sticker-percent"><p>Giảm ' + percent + '%</p></div>';
                                                }
                                                html += '<div class="item-product__box-name">';
                                                html += '<a href="' + url + '">';
                                                html += '<h3>' + name + '</h3>';
                                                html += '</a>';
                                                html += '</div>';
                                                if (key_selling_point) {
                                                    html += '<div class="item-product__more-info">';
                                                    html += '<p>' + key_selling_point + '</p>';
                                                    html += '</div>';
                                                }
